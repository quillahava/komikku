<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="WebviewPage" parent="AdwNavigationPage">
    <property name="tag">webview</property>
    <property name="title" translatable="yes">Webview</property>

    <property name="child">
      <object class="AdwToolbarView" id="toolbarview">
        <child type="top">
          <object class="AdwHeaderBar">
            <property name="title-widget">
              <object class="AdwWindowTitle" id="title" />
            </property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
